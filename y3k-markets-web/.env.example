# Copy to .env.local for local development
# Never commit real secrets.

# Public API base used by the Next.js frontend.
# Local dev example: http://127.0.0.1:8081
# Production example: https://api.y3kmarkets.com
NEXT_PUBLIC_API_URL=https://api.y3kmarkets.com

# Optional: IPFS gateway used for certificate links.
NEXT_PUBLIC_IPFS_GATEWAY=https://ipfs.io/ipfs/
